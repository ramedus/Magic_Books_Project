@model Magic_Books_Project.Models.Subcategory

@{
    Layout = null;
}
@Styles.Render("~/Content/css")


<h3>Are you sure you want to delete this?</h3>
<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            Subcategory Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.subcategory_name)
        </dd>

        <dt>
            Categories Name
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Categories.name)
        </dd>

    </dl>

    @using (Html.BeginForm())
    {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="site-btn" /> |
            @Html.ActionLink("Back to List", "subcategory_edit_delete")
        </div>
    }
</div>
